<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>

        <item name="back_ground_color">#B4AFAF</item>
        <item name="card_back_ground_color">#D5D2D2</item>
        <item name="tint_color">#686464</item>
        <item name="text_color">#888181</item>
    </style>


    <style name="DarkTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>

        <item name="back_ground_color">#686464</item>
        <item name="card_back_ground_color">#888181</item>
        <item name="tint_color">#ffffff</item>
        <item name="text_color">#ffffff</item>
    </style>


</resources>
